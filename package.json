{
  "name": "polyasync",
  "version": "1.2.0",
  "description": "Simple JS module for Promise and Fetch APIs, with some abstraction and polyfills to support all browsers",
  "main": "async.umd.js",
  "jsnext:main": "async.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexbruno/polyasync.git"
  },
  "keywords": [
    "async",
    "promise",
    "fetch",
    "javascript"
  ],
  "author": "Alex Bruno <master@conext.online>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexbruno/polyasync/issues"
  },
  "homepage": "https://github.com/alexbruno/polyasync#readme",
  "scripts": {
    "bundle": "jspm bundle-sfx async.js async.umd.js --format umd --skip-source-maps --minify",
    "postinstall": "jspm i && npm run bundle"
  },
  "devDependencies": {
    "jspm": "latest"
  },
  "jspm": {
    "dependencies": {
      "fetch": "github:github/fetch@^2.0.1",
      "promise": "github:taylorhakes/promise-polyfill@^6.0.2",
      "queryfetch": "npm:queryfetch@^1.1.1",
      "truetype": "npm:truetype@^1.2.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
