{
  "name": "@jsweb/asyncs",
  "version": "3.0.7",
  "description": "Simple JS module for Promise and Fetch APIs, with some useful abstraction",
  "scripts": {
    "test": "run-s test:*",
    "test:lint": "npm run lint",
    "test:clean": "shx rm -rf mocha*",
    "test:unit": "mocha src/**/*.test.ts --require ts-node/register --reporter mochawesome --reporter-options reportTitle=@jsweb/asyncs",
    "major": "npm version major && npm run push",
    "minor": "npm version minor && npm run push",
    "patch": "npm version patch && npm run push",
    "prepare": "npm run dist",
    "push": "git push && git push --tags",
    "lint": "tsc --noEmit && tslint -p . -c tslint.json --fix",
    "dist": "run-s dist:*",
    "dist:clean": "shx rm -rf dist",
    "dist:test": "npm t",
    "dist:build": "rollup -c",
    "dist:types": "tsc --removeComments --emitDeclarationOnly",
    "dist:docs": "npm run docs",
    "docs": "run-s docs:*",
    "docs:readme": "jsdoc2md dist/module.js -m 'none' > README.md",
    "docs:clean": "node ./scripts/readme.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsweb/asyncs.git"
  },
  "keywords": [
    "async",
    "promise",
    "fetch",
    "javascript"
  ],
  "author": "Alex Bruno <git.alexbr@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsweb/asyncs/issues"
  },
  "homepage": "https://github.com/jsweb/asyncs",
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.0.8",
    "babel-helper-evaluate-path": "^0.5.0",
    "husky": "^2.4.1",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^5.0.0",
    "mocha": "^6.1.4",
    "mochawesome": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.0.1",
    "rollup-plugin-esmin": "^0.1.3",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-typescript": "^1.0.0",
    "shx": "^0.3.2",
    "ts-node": "^8.3.0",
    "tslib": "^1.9.3",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@jsweb/params": "latest",
    "@jsweb/truetype": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run dist && git add ."
    }
  },
  "main": "dist/umd.js",
  "esnext": "dist/module.js",
  "module": "dist/module.js",
  "types": "dist/module.d.ts"
}
