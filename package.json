{
  "name": "@jsweb/asyncs",
  "version": "3.0.2",
  "description": "Simple JS module for Promise and Fetch APIs, with some useful abstraction",
  "main": "dist/umd.js",
  "esnext": "dist/module.js",
  "module": "dist/module.mjs",
  "types": "dist/module.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsweb/asyncs.git"
  },
  "keywords": [
    "async",
    "promise",
    "fetch",
    "javascript"
  ],
  "author": "Alex Bruno <git.alexbr@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsweb/asyncs/issues"
  },
  "homepage": "https://github.com/jsweb/asyncs",
  "scripts": {
    "dist": "run-s dist:*",
    "dist:clean": "shx rm -rf dist",
    "dist:test": "npm t",
    "dist:types": "tsc --removeComments --emitDeclarationOnly",
    "dist:build": "rollup -c",
    "dist:copy": "shx cp ./dist/module.mjs ./dist/module.js",
    "dist:docs": "npm run docs",
    "test": "run-s test:*",
    "test:lint": "tslint -p . -c tslint.json",
    "test:clean": "shx rm -rf mocha*",
    "test:unit": "mocha src/**/*.test.ts --require ts-node/register --reporter mochawesome --reporter-options reportTitle=@jsweb/asyncs",
    "docs": "run-s docs:*",
    "docs:readme": "jsdoc2md dist/module.js -m 'none' > README.md",
    "docs:clean": "node ./scripts/readme.js",
    "prepare": "npm run dist"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.65.2",
    "rollup-plugin-esmin": "^0.1.3",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript": "^1.0.0",
    "shx": "^0.3.2",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "@jsweb/params": "^3.1.2",
    "@jsweb/truetype": "^3.0.7"
  }
}
